{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "lib/libvosk.dll",
      "vulkan-1.dll"
    ]
  },
  "productName": "Tauri Whisper App",
  "mainBinaryName": "Tauri Whisper App",
  "version": "0.0.1",
  "identifier": "com.tauri-whisper.app",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "dragDropEnabled": true,
        "fullscreen": false,
        "resizable": true,
        "title": "Tauri Whisper App",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  }
}